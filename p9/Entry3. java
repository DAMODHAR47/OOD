package problem9;

interface Account{
	Dialog Create();
	...
}
class SA implements Account {
	Dialog Create(){
		return new SADialog();
	}
	...
}
class CA implements Account {
	Dialog Create(){
		return new CADialog();
	}
	...
}
public class Entry {
	public static void ShowUI(Account account) {
		Dialog dlg=account.Create(); //<-- factory method pattern
		dlg.Display();			
	}
	
	static void main()
	{
		Account account = new SA();
		ShowUI(account);
		
	}
}
